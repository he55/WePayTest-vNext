<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.hwz.wepayd</string>
	<key>Program</key>
	<string>/usr/sbin/wepayd</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/sbin/wepayd</string>
	</array>
	<key>EnvironmentVariables</key>
	<dict>
		<key>_WebServerPort</key>
		<string>5200</string>
	</dict>
	<key>UserName</key>
	<string>root</string>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
	<key>ThrottleInterval</key>
	<integer>5</integer>
	<key>ExecuteAllowed</key>
	<true/>
</dict>
</plist>
